{
  "manifest_version": 2,
  "name": "WebToEpub",
  "version": "0.0.0.29",
  "default_locale": "en",
  "icons": {
    "128": "book128.png"
  },
  "permissions": [
    "storage",
    "tabs", 
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "book128.png",
    "default_popup": "popup.html"
  },
  "applications": {
    "gecko": {
      "id": "WebToEpub@Baka-tsuki.org"
    }
  },
  "minimum_chrome_version": "46",
  "minimum_firefox_version": "48"
}
